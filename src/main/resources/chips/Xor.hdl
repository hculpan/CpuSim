///////////////////////////////////////
//  XOR chip
///////////////////////////////////////

// Define inputs and outputs
inputs(2)
outputs(1)

// Define chips
Chip c1 = Not()
Chip c2 = Not()
Chip c3 = And()
Chip c4 = And()
Chip c5 = Or()

// Wire inputs
wireInput(0, c2, 0)
wireInput(0, c3, 0)
wireInput(1, c1, 0)
wireInput(1, c4, 1)

// Internal wires
wire(c1, 0, c3, 1)
wire(c2, 0, c4, 0)
wire(c3, 0, c5, 0)
wire(c4, 0, c5, 1)

// Wire outputs
wireOutput(c5, 0, 0)

